{
  "manifest_version": 3,
  "name": "blocker",
  "description": "...",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon-16.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/block.js"],
      "matches": ["<all_urls>"],
      "all_frames": true
    }
  ],
   "background": {
    "service_worker": "background.js"
  }
}
